def $x plus $y {
  x + y;
};

def $x times $y {
  return x * y;
};

def print sum of $x and $y {
  print (x + y);
};

a = \5 times 10 * 10;
b = \(3 * 2) plus $a;

print sum of $a and $b;

# Define a block called a_block with no parametes
a_block = {
  print "Hello, world";
};

# Define a block called add with two parameters
add = { |a, b| -> a + b; };

# Execute the first block
run $a_block;

# Execute the second block with two arguments
run $add with [2, 3];